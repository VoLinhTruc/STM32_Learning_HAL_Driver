#ifndef INC_SNAKE_QUEUE_H_
#define INC_SNAKE_QUEUE_H_

#define SQ SQ
#define SQ_ELEMENT_TYPE uint8_t

typedef enum
{
	SQ_DEQUEUE_SUCCESS,
	SQ_NOT_AVAILABLE_TO_DEQUEUE
}SQ_Dequeue_State, SQ_Peek_State;

typedef struct
{
	SQ_ELEMENT_TYPE* SQ_array;
	uint32_t SQ_array_size;
	uint32_t SQ_head;
	uint32_t SQ_tail;
	uint32_t SQ_feed_buffer;
}SQ_t;

void				SQ(SQ_t* _sq, SQ_ELEMENT_TYPE*, _sq_array, uint32_t _sq_array_size);
void				SQEnqueue(SQ_ELEMENT_TYPE _element_in);
SQ_Dequeue_State	SQDequeue(SQ_ELEMENT_TYPE* _element_out);
uint32_t 			SQAvailable();
SQ_Peek_State		SQPeek(SQ_ELEMENT_TYPE* _element_out);
uint32_t			SQEnqueueAll(SQ_ELEMENT_TYPE* _element_out);


#endif /* INC_SNAKE_QUEUE_H_ */
